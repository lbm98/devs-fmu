loadFile(getInstallationDirectoryPath() + "/share/doc/omc/testmodels/BouncingBall.mo");

mkdir(
    newDirectory="/tmp/OpenModelica"
);

cd(
    newWorkingDirectory="/tmp/OpenModelica"
);

generatedFileName := translateModelFMU(
      className=BouncingBall,
      version="2.0",
      fmuType="cs"
);

mkdir(
    newDirectory="OpenModelica-FMUs"
);

copy(
    source=generatedFileName,
    destination="OpenModelica-FMUs/BouncingBall.fmu"
);